{
  "name": "@unicef/material-slate",
  "version": "1.0.0",
  "description": "Material UI rich text editor based on Slate for react",
  "main": "./dist/index.js",
  "homepage": "https://unicef.github.io/material-slate",
  "repository": {
    "type": "git",
    "url": "https://github.com/unicef/material-slate"
  },
  "scripts": {
    "start": "rollup -c -w",
    "docs": "npx styleguidist server",
    "build": "rollup -c",
    "build:docs": "npx styleguidist build",
    "build:site": "mkdir -p ./site && npm run build:docs && cp -R styleguide/ ./site/docs/ && cd example && npm run build && cp -R build/ ../site/",
    "deploy": "npm run build:site && gh-pages -d site",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "editor",
    "rich text",
    "text editor",
    "material-ui",
    "mui",
    "slate",
    "slatejs",
    "plugin",
    "rte",
    "react"
  ],
  "author": "UNICEF",
  "license": "MIT",
  "peerDependencies": {
    "@mui/material": ">= 5.0.0",
    "prop-types": ">= 15.0.0",
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.104.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/plugin-external-helpers": "^7.24.7",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.15.14",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-url": "^8.0.2",
    "@types/node": "^20.14.2",
    "babel-loader": "^9.1.3",
    "gh-pages": "^6.1.1",
    "is-hotkey": "^0.2.0",
    "is-url": "^1.2.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.1.2",
    "rollup": "^4.18.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.104.0",
    "webpack": "^5.64.4",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "react-is": "^18.2.0"
  }
}
